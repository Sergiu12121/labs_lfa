// NFA to DFA Conversion
digraph {
	"{'q0'}"
	"{'q1'}"
	"{'q1', 'q2'}"
	"{'q1', 'q3', 'q2'}"
	"{'q1', 'q4'}"
	"{'q0'}" -> "{'q1'}" [label=a]
	"{'q1'}" -> "{'q1', 'q2'}" [label=b]
	"{'q1', 'q2'}" -> "{'q4'}" [label=a]
	"{'q1', 'q2'}" -> "{'q1', 'q3', 'q2'}" [label=b]
	"{'q1', 'q3', 'q2'}" -> "{'q1', 'q4'}" [label=a]
	"{'q1', 'q3', 'q2'}" -> "{'q1', 'q3', 'q2'}" [label=b]
	"{'q1', 'q4'}" -> "{'q1', 'q2'}" [label=b]
}
